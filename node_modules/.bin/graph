#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ina/ina-subgraphs/node_modules/.pnpm/@graphprotocol+graph-cli@0.73.0_@types+node@22.13.1_encoding@0.1.13_node-fetch@2.7.0_encoding@0.1.13__typescript@5.7.3/node_modules/@graphprotocol/graph-cli/bin/node_modules:/home/ina/ina-subgraphs/node_modules/.pnpm/@graphprotocol+graph-cli@0.73.0_@types+node@22.13.1_encoding@0.1.13_node-fetch@2.7.0_encoding@0.1.13__typescript@5.7.3/node_modules/@graphprotocol/graph-cli/node_modules:/home/ina/ina-subgraphs/node_modules/.pnpm/@graphprotocol+graph-cli@0.73.0_@types+node@22.13.1_encoding@0.1.13_node-fetch@2.7.0_encoding@0.1.13__typescript@5.7.3/node_modules/@graphprotocol/node_modules:/home/ina/ina-subgraphs/node_modules/.pnpm/@graphprotocol+graph-cli@0.73.0_@types+node@22.13.1_encoding@0.1.13_node-fetch@2.7.0_encoding@0.1.13__typescript@5.7.3/node_modules:/home/ina/ina-subgraphs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ina/ina-subgraphs/node_modules/.pnpm/@graphprotocol+graph-cli@0.73.0_@types+node@22.13.1_encoding@0.1.13_node-fetch@2.7.0_encoding@0.1.13__typescript@5.7.3/node_modules/@graphprotocol/graph-cli/bin/node_modules:/home/ina/ina-subgraphs/node_modules/.pnpm/@graphprotocol+graph-cli@0.73.0_@types+node@22.13.1_encoding@0.1.13_node-fetch@2.7.0_encoding@0.1.13__typescript@5.7.3/node_modules/@graphprotocol/graph-cli/node_modules:/home/ina/ina-subgraphs/node_modules/.pnpm/@graphprotocol+graph-cli@0.73.0_@types+node@22.13.1_encoding@0.1.13_node-fetch@2.7.0_encoding@0.1.13__typescript@5.7.3/node_modules/@graphprotocol/node_modules:/home/ina/ina-subgraphs/node_modules/.pnpm/@graphprotocol+graph-cli@0.73.0_@types+node@22.13.1_encoding@0.1.13_node-fetch@2.7.0_encoding@0.1.13__typescript@5.7.3/node_modules:/home/ina/ina-subgraphs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphprotocol/graph-cli/bin/run" "$@"
else
  exec node  "$basedir/../@graphprotocol/graph-cli/bin/run" "$@"
fi
